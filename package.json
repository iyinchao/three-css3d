    "fix": "gts fix",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyinchao/three-css3d.git"
  },
  "keywords": [
    "three",
    "threejs",
    "3D",
    "css",
    "transform",
    "sprite",
    "renderer"
  ],
  "author": "iyinchao <yincharles@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyinchao/three-css3d/issues"
  },
  "homepage": "https://github.com/iyinchao/three-css3d#readme",
  "peerDependencies": {
    "three": ">=0.80.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/three": "^0.129.1",
    "gts": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "three": "^0.129.0",
    "typescript": "^4.0.3"
  }
}
